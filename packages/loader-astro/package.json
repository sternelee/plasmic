{
  "name": "@plasmicapp/loader-astro",
  "version": "1.0.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "yarn build:types && yarn build:index",
    "build:types": "yarn tsc",
    "build:index": "node ../../build.mjs ./src/index.tsx --use-client",
    "yalcp": "yalc publish --push",
    "test": "jest packages/loader-astro --config=../../jest.config.js --passWithNoTests",
    "coverage": "yarn test --coverage",
    "lint": "eslint",
    "prepublishOnly": "npm run build",
    "postpublish": "bash ../../scripts/publish-api-doc-model.sh",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "astro": ">=3.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": false
    }
  },
  "keywords": [
    "astro",
    "plasmic",
    "visual-builder",
    "cms",
    "headless-cms"
  ],
  "author": "Plasmic",
  "license": "MIT",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "15 KB"
    }
  ],
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "astro": "^4.0.0"
  },
  "dependencies": {
    "@plasmicapp/loader-core": "1.0.142",
    "@plasmicapp/loader-react": "1.0.410",
    "@plasmicapp/watcher": "1.0.84"
  }
}
